{
    "name": "neoblog-plugin-control-panel",
    "version": "0.1.0",
    "description": "article/config management panel for neoblog",
    "scripts": {
        "start": "neoblog",
        "build:dev": "cd src/client && elm-make Dev.elm --output ../../static/dev.html",
        "build:prod": "cd src/client && elm-make Main.elm --output ../../static/dist.js && google-closure-compiler-js ../../static/dist.js > ../../static/dist.min.js"
    },
    "keywords": [
        "neoblog",
        "neoblog-plugin"
    ],
    "author": "rocka <i@rocka.me>",
    "license": "MIT",
    "dependencies": {
        "jsonwebtoken": "^8.1.1",
        "koa-body": "^2.5.0",
        "koa-jwt": "^3.3.0",
        "koa-mount": "^3.0.0",
        "koa-router": "^7.4.0",
        "koa-send": "^4.1.2",
        "koa-static-cache": "^5.1.1"
    },
    "devDependencies": {
        "elm": "^0.18.0",
        "google-closure-compiler-js": "^20180101.0.0",
        "neoblog": "git+https://github.com/rocket1184/neoblog.git"
    }
}
